# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  about: AboutQuery
  blog: BlogQuery
  experience: ExperienceQuery
  resume: ResumeQuery
  showcase: ShowcaseQuery
}

type Mutation {
  about: AboutMutation
  blog: BlogMutation
  experience: ExperienceMutation
  resume: ResumeMutation
  showcase: ShowcaseMutation
}







